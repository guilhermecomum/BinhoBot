{
  "name": "binhobot",
  "description": "My personal assistant bot",
  "version": "1.0.0",
  "engines": {
    "node": "12.0.0"
  },
  "main": "main.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/src/main.js",
    "devel": "nodemon --exec babel-node -- src/main.jsx"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^4.0.0",
    "github-webhook-handler": "^0.6.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "spotify-web-api-node": "^4.0.0",
    "telebot": "^1.2.3",
    "telegraf": "^3.29.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guilhermecomum/BinhoBot.git"
  },
  "author": "Guilherme Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guilhermecomum/BinhoBot/issues"
  },
  "homepage": "https://github.com/guilhermecomum/BinhoBot#readme"
}
